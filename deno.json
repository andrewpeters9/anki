{
  "nodeModulesDir": true,
  "lock": "package-lock.json",
  "tasks": {
    "math-jax": "deno run --allow-read --allow-write --allow-env --allow-net --allow-run  ts/transform_ts.ts"
  },
  "compilerOptions": {
    "isolatedModules": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns", "deno.window", "deno.worker"],
    "strict": true,
    "noImplicitAny": false,
    "alwaysStrict": true,
    "noUncheckedIndexedAccess": true,
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react"
  }
}
